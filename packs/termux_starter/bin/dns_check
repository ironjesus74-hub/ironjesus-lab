#!/data/data/com.termux/files/usr/bin/bash
source "$HOME/ironjesus-lab/lib/common.sh"
color blue "[*] DNS info"
getprop net.dns1 2>/dev/null || true
getprop net.dns2 2>/dev/null || true
echo "--- resolv.conf (if present) ---"
cat /etc/resolv.conf 2>/dev/null || true
need ping
ping -c 1 -W 2 google.com >/dev/null 2>&1 && color green "[+] DNS OK" || color yellow "[!] DNS lookup failed"
log "dns_check ran"
